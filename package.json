{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"tsx watch src/services/telegram-bot.ts\"",
    "build": "tsc && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "build:all": "npm run build && npm run build:server",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start:api": "node dist/services/api-server.js",
    "start:bot": "node dist/services/telegram-bot.js",
    "start:web": "node server.js",
    "start": "node unified-server.js",
    "start:dev": "concurrently \"npm run start:api\" \"npm run start:bot\" \"npm run start:web\"",
    "api": "node api-server.js",
    "simple-api": "node simple-api-server.js",
    "generate-cookies": "node scripts/generate-cookies.js",
    "list-cookies": "node scripts/manage-cookies.js",
    "gen-cookies": "node scripts/generate-cookies.js",
    "setup": "mkdir -p valid-cookies cookie-backups && chmod 755 valid-cookies cookie-backups",
    "generate-icons": "node scripts/generate-icons.js",
    "analyze": "vite build --mode analyze",
    "optimize-images": "node scripts/optimize-images.js",
    "build:prod": "npm run optimize-images && vite build --mode production"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.6.5",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.344.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.3",
    "telegraf": "^4.15.3",
    "tsx": "^4.7.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/adm-zip": "^0.5.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.9.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.0.8"
  }
}
